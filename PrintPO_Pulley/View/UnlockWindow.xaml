<Window x:Class="BFDataCrawler.View.UnlockWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BFDataCrawler.View"
        mc:Ignorable="d"
        WindowStyle="ToolWindow"
        WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize"
        Title="Nhập mật khẩu" Height="80" Width="400"
        Loaded="Window_Loaded">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <PasswordBox x:Name="txtPass" PasswordChanged="PasswordBox_PasswordChanged" KeyDown="PasswordBox_KeyDown" BorderBrush="#123" BorderThickness="1" Padding="4" FontSize="16" VerticalContentAlignment="Center" Height="50" Width="396"/>
        </StackPanel>
    </Grid>
</Window>
